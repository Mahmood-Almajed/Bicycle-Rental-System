<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <%- include('../partials/nav.ejs') %>
    <h1>Your Rental Bike</h1>
    <a href="/users/<%= user._id %>/rental/new">Rental new Bike</a>
    <ul>
      <% bookings.forEach( booking => { %>
      <li>
        <h2><%= booking.bike[0].company %> - <%= booking.bike[0].type %></h2>
      </li>
      <p>Address: <%= booking.adders %></p>
      <p>Start date: <%= booking.fristdate.toISOString().split('T')[0] %></p>
      <p>End date: <%= booking.lastdate.toISOString().split('T')[0] %></p>
      <a href="/users/<%= user._id %>/rental/<%= booking._id %>">
        View details</a
      >
      <% }) %>
    </ul>
  </body>
</html>
